{% extends 'base.html.twig' %}

{% block header %}
    {% if users is iterable %}
        {% for user in users %}
            Hello {{ user.name }}
        {% endfor %}
    {% else %}
        Hello {{ users }}
    {% endif %}
{% endblock %}

{% block body %}
    {{  controller_name }}
    <ul>
        {% for user in users %}
            <li>{{ user.name }}</li>
        {% endfor %}
    </ul>
{% endblock %}
